{
    "name": "无尽冬日助手",
    "interface_version": 2,
    "github": "https://github.com/l0rraine/MWJDR",
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        }
    ],
    "agent": {
        "child_exec": "{PROJECT_DIR}/python/python.exe",
        "child_args": [
            "-u",
            "{PROJECT_DIR}/../agent/main.py"
        ]
    },
    "task": [
        {
            "name": "启动游戏",
            "entry": "启动游戏",
            "option": [
                "切换角色",
                "确保有队伍可用"
            ]
        },
        {
            "name": "自动灯塔",
            "entry": "灯塔入口",
            "option": [
                "灯塔出征队伍设置"
            ]
        },
        {
            "name": "自动集结",
            "entry": "自动集结_巨兽入口",
            "option": [
                "巨兽队伍设置",
                "巨兽战斗设置"
            ]
        },
        {
            "name": "自动吉娜",
            "entry": "自动集结_吉娜入口",
            "option": [
                "吉娜队伍设置",
                "吉娜战斗设置"
            ]
        },
        {
            "name": "自动野兽",
            "entry": "自动野兽_入口",
            "option": [
                "野兽队伍设置"
            ]
        },
        {
            "name": "联盟总动员",
            "entry": "联盟总动员_入口",
            "option": [
                "是否启用第一栏位",
                "是否启用第二栏位",
                "训练士兵-12万",
                "专精石-35",
                "领主装备-8900",
                "钻石-50000",
                "礼包-648",
                "礼包-328"
            ]
        },
        {
            "name": "半夜挖矿",
            "entry": "挖矿_入口",
            "option": [
                "先采联盟矿",
                "保留自动加入"
            ]
        },
        {
            "name": "结束",
            "entry": "结束_入口",
            "option": [
                "打开自动集结"
            ]
        }
    ],
    "option": {
        "确保有队伍可用": {
            "type": "switch",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {}
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "确保有空闲队列": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "打开自动集结": {
            "type": "switch",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "自动加入集结_已关闭自动加入": {
                            "enabled": true
                        },
                        "自动加入集结_已打开自动加入": {
                            "enabled": false
                        }
                    }
                },
                {
                    "name": "No",
                    "pipeline_override": {}
                }
            ]
        },
        "先采联盟矿": {
            "type": "switch",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {}
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "挖矿_参数_联盟矿": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "保留自动加入": {
            "type": "switch",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {}
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "挖矿_参数_保留自动加入": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "是否启用第一栏位": {
            "type": "switch",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {}
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "联盟总动员_参数_是否启用第一栏位": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "是否启用第二栏位": {
            "type": "switch",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {}
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "联盟总动员_参数_是否启用第二栏位": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "训练士兵-12万": {
            "type": "switch",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {}
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "联盟总动员_任务_训练士兵-12w": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "专精石-35": {
            "type": "switch",
            "cases": [
                {
                    "name": "No",
                    "pipeline_override": {
                        "联盟总动员_任务_专精石-35": {
                            "enabled": false
                        }
                    }
                },
                {
                    "name": "Yes",
                    "pipeline_override": {}
                }
            ]
        },
        "领主装备-8900": {
            "type": "switch",
            "cases": [
                {
                    "name": "No",
                    "pipeline_override": {
                        "联盟总动员_任务_领主装备-8900": {
                            "enabled": false
                        }
                    }
                },
                {
                    "name": "Yes",
                    "pipeline_override": {}
                }
            ]
        },
        "礼包-648": {
            "type": "switch",
            "cases": [
                {
                    "name": "No",
                    "pipeline_override": {
                        "联盟总动员_任务_648礼包": {
                            "enabled": false
                        }
                    }
                },
                {
                    "name": "Yes",
                    "pipeline_override": {}
                }
            ]
        },
        "礼包-328": {
            "type": "switch",
            "cases": [
                {
                    "name": "No",
                    "pipeline_override": {
                        "联盟总动员_任务_328礼包": {
                            "enabled": false
                        }
                    }
                },
                {
                    "name": "Yes",
                    "pipeline_override": {}
                }
            ]
        },
        "钻石-50000": {
            "type": "switch",
            "cases": [
                {
                    "name": "No",
                    "pipeline_override": {
                        "联盟总动员_任务_50000钻石": {
                            "enabled": false
                        }
                    }
                },
                {
                    "name": "Yes",
                    "pipeline_override": {}
                }
            ]
        },
        "切换角色": {
            "type": "switch",
            "cases": [
                {
                    "name": "No",
                    "pipeline_override": {
                        "开始切换角色": {
                            "enabled": false
                        }
                    }
                },
                {
                    "name": "Yes",
                    "option": [
                        "角色信息"
                    ]
                }
            ]
        },
        "角色信息": {
            "type": "input",
            "label": "角色信息",
            "inputs": [
                {
                    "name": "王国编号",
                    "pipeline_type": "string",
                    "default": "3194",
                    "verify": "^(0|[1-9]\\d*)$"
                },
                {
                    "name": "王国内序号",
                    "pipeline_type": "string",
                    "default": "1",
                    "verify": "^[1-2]$"
                }
            ],
            "pipeline_override": {
                "确定角色": {
                    "action": {
                        "param": {
                            "custom_action_param": {
                                "王国编号": "{王国编号}",
                                "王国内序号": "{王国内序号}"
                            }
                        }
                    }
                }
            }
        },
        "巨兽队伍设置": {
            "type": "input",
            "label": "队伍设置",
            "inputs": [
                {
                    "name": "队伍序号",
                    "pattern_msg": "请输入队伍序号，0表示默认队伍",
                    "pipeline_type": "string",
                    "default": "0",
                    "verify": "^(\\d+)$",
                    "description": "请输入队伍序号，0表示默认队伍"
                }
            ],
            "pipeline_override": {
                "自动集结_选择队伍": {
                    "action": {
                        "param": {
                            "custom_action_param": {
                                "队伍序号": "{队伍序号}"
                            }
                        }
                    }
                }
            }
        },
        "巨兽战斗设置": {
            "type": "select",
            "cases": [
                {
                    "name": "清空体力",
                    "pipeline_override": {
                        "自动集结_准备出征": {
                            "custom_action_param": {
                                "出征次数": "-1",
                                "巨兽种类": 0
                            }
                        }
                    }
                },
                {
                    "name": "智能(未实现)",
                    "pipeline_override": {
                        "自动集结_准备出征": {
                            "custom_action_param": {
                                "出征次数": "0",
                                "巨兽种类": 0
                            }
                        }
                    }
                }
            ]
        },
        "吉娜队伍设置": {
            "type": "input",
            "label": "队伍设置",
            "inputs": [
                {
                    "name": "队伍序号",
                    "pattern_msg": "请输入队伍序号，0表示默认队伍",
                    "pipeline_type": "string",
                    "default": "0",
                    "verify": "^(\\d+)$",
                    "description": "请输入队伍序号，0表示默认队伍"
                }
            ],
            "pipeline_override": {
                "自动吉娜_选择队伍": {
                    "action": {
                        "param": {
                            "custom_action_param": {
                                "队伍序号": "{队伍序号}"
                            }
                        }
                    }
                }
            }
        },
        "吉娜战斗设置": {
            "type": "select",
            "cases": [
                {
                    "name": "清空体力",
                    "pipeline_override": {
                        "自动吉娜_准备出征": {
                            "custom_action_param": {
                                "出征次数": "-1",
                                "巨兽种类": 1
                            }
                        }
                    }
                },
                {
                    "name": "智能(未实现)",
                    "pipeline_override": {
                        "自动吉娜_准备出征": {
                            "custom_action_param": {
                                "出征次数": "0",
                                "巨兽种类": 1
                            }
                        }
                    }
                }
            ]
        },
        "灯塔出征队伍设置": {
            "type": "input",
            "label": "队伍设置",
            "inputs": [
                {
                    "name": "队伍序号",
                    "pattern_msg": "请输入队伍序号，0表示默认队伍",
                    "pipeline_type": "string",
                    "default": "0",
                    "verify": "^(\\d+)$",
                    "description": "请输入队伍序号，0表示默认队伍"
                }
            ],
            "pipeline_override": {
                "灯塔选择队伍": {
                    "action": {
                        "param": {
                            "custom_action_param": {
                                "队伍序号": "{队伍序号}"
                            }
                        }
                    }
                }
            }
        },
        "野兽队伍设置": {
            "type": "input",
            "label": "队伍设置",
            "inputs": [
                {
                    "name": "队伍序号",
                    "pattern_msg": "请输入队伍序号，0表示默认队伍",
                    "pipeline_type": "string",
                    "default": "0",
                    "verify": "^(\\d+)$",
                    "description": "请输入队伍序号，0表示默认队伍"
                }
            ],
            "pipeline_override": {
                "自动野兽_选择队伍": {
                    "action": {
                        "param": {
                            "custom_action_param": {
                                "队伍序号": "{队伍序号}"
                            }
                        }
                    }
                }
            }
        }
    }
}