{
    "name": "无尽冬日小助手",
    "url": "https://github.com/l0rraine/MWJDR",
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        }
    ],
    "agent": {
        "child_exec": "{PROJECT_DIR}/python/python.exe",
        "child_args": [
            "-u",
            "{PROJECT_DIR}/../agent/main.py"
        ]
    },
    "task": [
        {
            "name": "启动游戏",
            "entry": "启动游戏",
            "option": [
                "王国内序号"
            ]
        },
        {
            "name": "自动灯塔",
            "entry": "灯塔入口",
            "option": [
                "灯塔出征队伍设置"
            ]
        },
        {
            "name": "自动集结",
            "entry": "自动集结_巨兽入口",
            "option": [
                "巨兽队伍设置",
                "巨兽战斗设置"
            ]
        },
        {
            "name": "自动吉娜",
            "entry": "自动集结_吉娜入口",
            "option": [
                "吉娜队伍设置",
                "吉娜战斗设置"
            ]
        },
        {
            "name": "自动野兽",
            "entry": "自动野兽_入口",
            "option": [
                "野兽队伍设置"
            ]
        },
        {
            "name": "联盟总动员",
            "entry": "联盟总动员_入口",
            "option": [
                "是否只要双倍",
                "训练士兵-12万-200%",
                "训练士兵-12万-120%",
                "648礼包-200%",
                "648礼包-120%",
                "328礼包-200%",
                "328礼包-120%"
            ]
        },
        {
            "name": "半夜挖矿",
            "entry": "挖矿_入口",
            "option": [
                "联盟矿",
                "保留自动加入"
            ]
        }
    ],
    "option": {
        "先采联盟矿": {
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {}
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "挖矿_参数_联盟矿": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "保留自动加入": {
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {}
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "挖矿_参数_保留自动加入": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "训练士兵-12万-200%": {
            "cases": [
