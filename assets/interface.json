{
    "interface_version": 2,
    "name": "MWJDR",
    "github": "https://github.com/l0rraine/MWJDR",
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        }
    ],
    "agent": {
        "child_exec": "{PROJECT_DIR}/python/python.exe",
        "child_args": [
            "-u",
            "{PROJECT_DIR}/../agent/main.py"
        ]
    },
    "task": [
        {
            "name": "启动游戏",
            "entry": "启动游戏",
            "option": [
                "切换角色",
                "确保有队伍可用"
            ]
        },
        {
            "name": "自动灯塔",
            "entry": "灯塔入口",
            "option": [
                "灯塔出征队伍设置"
            ]
        },
        {
            "name": "集结巨兽",
            "entry": "自动集结_巨兽入口",
            "option": [
                "巨兽队伍设置",
                "巨兽作战设置"
            ]
        },
        {
            "name": "集结吉娜",
            "entry": "自动集结_吉娜_识别体力入口",
            "option": [
                "吉娜队伍设置",
                "吉娜体力设置"
            ]
        },
        {
            "name": "自动野兽",
            "entry": "自动野兽_入口",
            "option": [
                "野兽队伍设置"
            ]
        },
        {
            "name": "联盟总动员",
            "entry": "联盟总动员_入口",
            "option": [
                "是否启用第一栏位",
                "是否启用第二栏位",
                "训练士兵-12万",
                "专精石-35",
                "领主装备-8900",
                "钻石-50000",
                "礼包-648",
                "礼包-328"
            ]
        },
        {
            "name": "自动游历",
            "entry": "自动游历_入口"
        },
        {
            "name": "结束",
            "entry": "结束_入口",
            "option": [
                "打开自动集结"
            ]
        }
    ],
    "option": {
        "确保有队伍可用": {
            "type": "switch",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {}
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "确保有空闲队列": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "打开自动集结": {
            "type": "switch",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "自动加入集结_已关闭自动加入": {
                            "enabled": true
                        },
                        "自动加入集结_已打开自动加入": {
                            "enabled": false
                        }
                    }
                },
                {
                    "name": "No",
                    "pipeline_override": {}
                }
            ]
        },
        "先采联盟矿": {
            "type": "switch",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {}
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "挖矿_参数_联盟矿": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "保留自动加入": {
            "type": "switch",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {}
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "挖矿_参数_保留自动加入": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "是否启用第一栏位": {
            "type": "switch",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {}
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "联盟总动员_参数_是否启用第一栏位": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "是否启用第二栏位": {
            "type": "switch",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {}
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "联盟总动员_参数_是否启用第二栏位": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "训练士兵-12万": {
            "type": "switch",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {}
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "联盟总动员_任务_训练士兵-12w": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "专精石-35": {
            "type": "switch",
            "cases": [
                {
                    "name": "No",
                    "pipeline_override": {
                        "联盟总动员_任务_专精石-35": {
                            "enabled": false
                        }
                    }
                },
                {
                    "name": "Yes",
                    "pipeline_override": {}
                }
            ]
        },
        "领主装备-8900": {
            "type": "switch",
            "cases": [
                {
                    "name": "No",
                    "pipeline_override": {
                        "联盟总动员_任务_领主装备-8900": {
                            "enabled": false
                        }
                    }
                },
                {
                    "name": "Yes",
                    "pipeline_override": {}
                }
            ]
        },
        "礼包-648": {
            "type": "switch",
            "cases": [
                {
                    "name": "No",
                    "pipeline_override": {
                        "联盟总动员_任务_648礼包": {
                            "enabled": false
                        }
                    }
                },
                {
                    "name": "Yes",
                    "pipeline_override": {}
                }
            ]
        },
        "礼包-328": {
            "type": "switch",
            "cases": [
                {
                    "name": "No",
                    "pipeline_override": {
                        "联盟总动员_任务_328礼包": {
                            "enabled": false
                        }
                    }
                },
                {
                    "name": "Yes",
                    "pipeline_override": {}
                }
            ]
        },
        "钻石-50000": {
            "type": "switch",
            "cases": [
                {
                    "name": "No",
                    "pipeline_override": {
                        "联盟总动员_任务_50000钻石": {
                            "enabled": false
                        }
                    }
                },
                {
                    "name": "Yes",
                    "pipeline_override": {}
                }
            ]
        },
        "切换角色": {
            "type": "switch",
            "label": "设置角色",
            "description": "关闭时为使用当前角色，打开时会检查当前是否为特定角色",
            "cases": [
                {
                    "name": "No",
                    "pipeline_override": {
                        "开始切换角色": {
                            "enabled": false
                        }
                    }
                },
                {
                    "name": "Yes",
                    "option": [
                        "角色信息"
                    ]
                }
            ]
        },
        "角色信息": {
            "type": "input",
            "label": "角色信息",
            "inputs": [
                {
                    "name": "王国编号",
                    "pipeline_type": "string",
                    "default": "3194",
                    "verify": "^(0|[1-9]\\d*)$"
                },
                {
                    "name": "王国内序号",
                    "pipeline_type": "string",
                    "default": "1",
                    "verify": "^[1-2]$"
                }
            ],
            "pipeline_override": {
                "确定角色": {
                    "action": {
                        "param": {
                            "custom_action_param": {
                                "王国编号": "{王国编号}",
                                "王国内序号": "{王国内序号}"
                            }
                        }
                    }
                }
            }
        },
        "巨兽队伍设置": {
            "type": "input",
            "label": "队伍设置",
            "inputs": [
                {
                    "name": "队伍序号",
                    "pattern_msg": "请输入队伍序号，0表示默认队伍",
                    "pipeline_type": "string",
                    "default": "0",
                    "verify": "^(\\d+)$",
                    "description": "请输入队伍序号，0表示默认队伍"
                }
            ],
            "pipeline_override": {
                "自动集结_选择队伍": {
                    "action": {
                        "param": {
                            "custom_action_param": {
                                "队伍序号": "{队伍序号}"
                            }
                        }
                    }
                }
            }
        },
        "巨兽作战设置": {
            "type": "switch",
            "label": "作战设置",
            "cases": [
                {
                    "name": "No",
                    "pipeline_override": {},
                    "option": [
                        "巨兽作战-多余体力吉娜"
                    ]
                },
                {
                    "name": "Yes",
                    "option": [
                        "巨兽作战高级设置"
                    ]
                }
            ],
            "description": "关闭时为普通模式，清空所有体力集结，打开时为高级模式"
        },
        "巨兽作战-多余体力吉娜": {
            "type": "switch",
            "label": "10次巨兽后打吉娜",
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "自动集结_准备出征": {
                            "custom_action_param": {
                                "出征次数": "10",
                                "吉娜": 1,
                                "罐头数量": 0
                            }
                        },
                        "自动集结_查看次数": {
                            "enabled": true
                        }
                    },
                    "option": [
                        "吉娜体力设置"
                    ]
                },
                {
                    "name": "No",
                    "pipeline_override": {}
                }
            ],
            "description": "每天只打10次巨兽，打完怪兽多余体力打吉娜"
        },
        "巨兽作战高级设置": {
            "type": "switch",
            "label": "高级设置",
            "cases": [
                {
                    "name": "No",
                    "option": [
                        "巨兽次数设置-精确次数"
                    ]
                },
                {
                    "name": "Yes",
                    "option": [
                        "巨兽体力设置-罐头个数"
                    ]
                }
            ],
            "description": "打开时为体力模式，关闭为次数模式"
        },
        "巨兽体力设置-罐头个数": {
            "type": "input",
            "inputs": [
                {
                    "name": "使用罐头个数",
                    "pattern_msg": "输入罐头个数",
                    "pipeline_type": "string",
                    "default": "0",
                    "verify": "^\\d+$",
                    "description": "输入罐头个数"
                }
            ],
            "pipeline_override": {
                "自动集结_准备出征": {
                    "custom_action_param": {
                        "出征次数": 0,
                        "吉娜": 0,
                        "罐头数量": "{使用罐头个数}"
                    }
                }
            }
        },
        "巨兽次数设置-精确次数": {
            "type": "input",
            "inputs": [
                {
                    "name": "战斗次数",
                    "pattern_msg": "输入打巨兽的次数，最多4位",
                    "pipeline_type": "string",
                    "default": "0",
                    "verify": "^[1-9]\\d{0,3}$",
                    "description": "输入打巨兽的次数，最多4位"
                }
            ],
            "pipeline_override": {
                "自动集结_准备出征": {
                    "custom_action_param": {
                        "出征次数": "{战斗次数}",
                        "吉娜": 0,
                        "罐头数量": 0
                    }
                }
            }
        },
        "吉娜体力设置": {
            "type": "input",
            "label": "体力设置",
            "inputs": [
                {
                    "name": "吉娜出征需要体力",
                    "pattern_msg": "出征吉娜消耗的体力",
                    "pipeline_type": "string",
                    "default": "20",
                    "verify": "^\\d+$",
                    "description": "出征吉娜消耗的体力"
                }
            ],
            "pipeline_override": {
                "自动集结_吉娜_识别体力": {
                    "custom_action_param": {
                        "体力消耗": "{吉娜出征需要体力}"
                    }
                }
            }
        },
        "吉娜队伍设置": {
            "type": "input",
            "label": "队伍设置",
            "inputs": [
                {
                    "name": "队伍序号",
                    "pattern_msg": "请输入队伍序号，0表示默认队伍",
                    "pipeline_type": "string",
                    "default": "0",
                    "verify": "^\\d+$",
                    "description": "请输入队伍序号，0表示默认队伍"
                }
            ],
            "pipeline_override": {
                "自动吉娜_选择队伍": {
                    "action": {
                        "param": {
                            "custom_action_param": {
                                "队伍序号": "{队伍序号}"
                            }
                        }
                    }
                }
            }
        },
        "灯塔出征队伍设置": {
            "type": "input",
            "label": "队伍设置",
            "inputs": [
                {
                    "name": "队伍序号",
                    "pattern_msg": "请输入队伍序号，0表示默认队伍",
                    "pipeline_type": "string",
                    "default": "0",
                    "verify": "^(\\d+)$",
                    "description": "请输入队伍序号，0表示默认队伍"
                }
            ],
            "pipeline_override": {
                "灯塔选择队伍": {
                    "action": {
                        "param": {
                            "custom_action_param": {
                                "队伍序号": "{队伍序号}"
                            }
                        }
                    }
                }
            }
        },
        "野兽队伍设置": {
            "type": "input",
            "label": "队伍设置",
            "inputs": [
                {
                    "name": "队伍序号",
                    "pattern_msg": "请输入队伍序号，0表示默认队伍",
                    "pipeline_type": "string",
                    "default": "0",
                    "verify": "^(\\d+)$",
                    "description": "请输入队伍序号，0表示默认队伍"
                }
            ],
            "pipeline_override": {
                "自动野兽_选择队伍": {
                    "action": {
                        "param": {
                            "custom_action_param": {
                                "队伍序号": "{队伍序号}"
                            }
                        }
                    }
                }
            }
        }
    }
}